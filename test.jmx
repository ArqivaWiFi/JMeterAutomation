        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="globals" enabled="true">
          <stringProp name="BeanShellSampler.query">import java.net.URLEncoder;

// Change for staging/live
vars.put(&quot;captive_portal_domain&quot;, props.get(&quot;captive_portal.domain&quot;));

// Site-specific configuration
vars.put(&quot;site_nasid&quot;, props.get(&quot;site.nasid&quot;));

vars.put(&quot;site_ssid&quot;, props.get(&quot;site.ssid&quot;));

vars.put(&quot;site_domain&quot;, props.get(&quot;site.domain&quot;));

vars.put(&quot;site_heading&quot;, props.get(&quot;site.heading&quot;));

vars.put(&quot;site_whitelist_example_domain&quot;, props.get(&quot;site.whitelist.example.domain&quot;));
vars.put(&quot;site_whitelist_example_path&quot;, props.get(&quot;site.whitelist.example.path&quot;));
vars.put(&quot;site_whitelist_example_expected_substring&quot;, props.get(&quot;site.whitelist.example.expected_substring&quot;));

vars.put(&quot;site_non_whitelist_example_domain&quot;, props.get(&quot;site.non_whitelist.example.domain&quot;));
vars.put(&quot;site_non_whitelist_example_path&quot;, props.get(&quot;site.non_whitelist.example.path&quot;));
vars.put(&quot;site_non_whitelist_example_expected_substring&quot;, props.get(&quot;site.non_whitelist.example.expected_substring&quot;));

// This must be either &quot;public&quot; or &quot;private&quot;
vars.put(&quot;site_blacklist_type&quot;, props.get(&quot;site.blacklist.type&quot;));

vars.put(&quot;site_payment_method_free_mins&quot;, props.get(&quot;site.payment_method.free.mins&quot;));

// The MAC address of the machine the tests are running from. Use uppercase!
vars.put(&quot;mac_address&quot;, props.get(&quot;mac_address&quot;));


//// Other constants ////

// This must always be blocked
vars.put(&quot;blacklist_universal_example_domain&quot;, &quot;anonymizer.com&quot;);
vars.put(&quot;blacklist_universal_example_path&quot;, &quot;/&quot;);
vars.put(&quot;blacklist_universal_example_content_substring&quot;, &quot;The document has moved&quot;);
vars.put(&quot;blacklist_universal_example_expected_substring&quot;, &quot;FIXME&quot;);

// This must ONLY be blocked if site_blacklist_type == &quot;public&quot;
vars.put(&quot;blacklist_public_example_domain&quot;, &quot;porn.com&quot;);
vars.put(&quot;blacklist_public_example_path&quot;, &quot;/&quot;);
vars.put(&quot;blacklist_public_example_content_substring&quot;, &quot;Free porn mega site&quot;);
vars.put(&quot;blacklist_public_example_expected_substring&quot;, &quot;FIXME&quot;);

//// Which payment method are we testing?
// Must be one of: { &quot;free&quot;, &quot;card&quot;, &quot;paypal&quot;, &quot;roaming&quot; }
vars.put(&quot;payment_method&quot;, props.get(&quot;payment_method&quot;));


//// Computed vars ////

vars.put(&quot;device_id&quot;, vars.get(&quot;mac_address&quot;).replace(&quot;:&quot;, &quot;&quot;));

vars.put(&quot;redirect_url&quot;,
  &quot;https://&quot; + vars.get(&quot;captive_portal_domain&quot;) + &quot;/?&quot; +
  &quot;aa[ac_url]=https%3A%2F%2F&quot; + vars.get(&quot;site_domain&quot;) + &quot;%2Flogin&quot; +
  &quot;&amp;aa[mac]=&quot; + java.net.URLEncoder.encode(vars.get(&quot;mac_address&quot;)) +
  &quot;&amp;aa[nasid]=&quot; + java.net.URLEncoder.encode(vars.get(&quot;site_nasid&quot;)) +
  &quot;&amp;aa[ssid]=&quot; + java.net.URLEncoder.encode(vars.get(&quot;site_ssid&quot;)) +
  &quot;&amp;aa[orig_url]=http%3A%2F%2F&quot; + java.net.URLEncoder.encode(vars.get(&quot;site_non_whitelist_example_domain&quot;)) + &quot;%2F&quot;);
  </stringProp>
